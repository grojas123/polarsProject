   "source": [
    "import polars as pl\n",
    "\n",
    "# Create a Polars DataFrame\n",
    "df = pl.DataFrame({\n",
    "    \"a\": [1, 2, 3],\n",
    "    \"b\": [4, 5, 6]\n",
    "})\n",
    "\n",
    "# Perform element-wise multiplication and sum the result\n",
    "dot_product = (df[\"a\"] * df[\"b\"]).sum()\n",
    "\n",
    "# Print the result\n",
    "print(\"Dot Product:\", dot_product)\n"
   ],
